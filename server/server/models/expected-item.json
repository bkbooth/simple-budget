  "options": {
    "validateUpsert": true,
    "postgresql": {
      "table": "expected_item"
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4"
    },
    "itemType": {
      "type": "string",
      "required": true,
      "postgresql": {
        "columnName": "item_type"
      }
    },
    "name": {
      "type": "string",
      "required": true
    },
    "period": {
      "type": "string",
      "required": true
    },
    "value": {
      "type": "number",
      "required": true
    },
    "dueDate": {
      "type": "date",
      "postgresql": {
        "columnName": "due_date"
      }
    },
    "archived": {
      "type": "boolean",
      "default": false
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    },
    "modified": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
